{
  "name": "@mockfetch/mockfetch",
  "version": "0.2.0",
  "description": "Mock the fetch function in the simplest way, including streaming. The interface is similar to the venerable mockjax, just using modern features like URL, URLSearchParams and URLPattern.",
  "author": {
    "name": "Ferdinand Prantl",
    "email": "prantlf@gmail.com",
    "url": "http://prantl.tk"
  },
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/prantlf/mockfetch/blob/master/LICENSE"
    }
  ],
  "homepage": "https://github.com/prantlf/mockfetch#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prantlf/mockfetch.git"
  },
  "bugs": {
    "url": "https://github.com/prantlf/mockfetch/issues"
  },
  "type": "module",
  "module": "dist/index.js",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "exports": {
    "require": "./dist/index.cjs",
    "import": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c && cp-j -R src/types/ dist/types/",
    "clean": "rm-j dist/*",
    "lint": "biome lint",
    "test:cjs": "node test/index.cjs",
    "test:ts": "tsc --noEmit test/types.ts",
    "test:js": "node test",
    "test": "npm run test:cjs && npm run test:ts && npm run test:js"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.6",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.1.4",
    "@unixcompat/cp.js": "^3.0.0",
    "@unixcompat/rm.js": "^3.0.0",
    "mockfetch": "link:",
    "rollup": "^4.52.4",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-swc-minify": "^1.3.0",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "urlpattern-polyfill": "^10.1.0"
  },
  "keywords": [
    "fetch",
    "mock",
    "streaming",
    "mockjax"
  ]
}